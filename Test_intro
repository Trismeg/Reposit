\vspace{1cm}

$$r=\scriptr \ \sin \phi $$
$$z=\scriptr \ \cos \phi $$
$$x=\scriptr\  \sin \phi\ \cos\theta$$
$$y=\scriptr\  \sin \phi\ \sin\theta$$

\newpage

\section{Vector Operations}
\subsection{Dot Product}
$$\overrightarrow{\scriptr_1}\cdot \overrightarrow{\scriptr_2}=\left(\begin{array}{c} x_1 \\ y_1 \\ z_1\end{array}\right)\left(\begin{array}{ccc} x_2 & y_2 & z_2\end{array}\right)= x_1x_2+y_1y_2+z_1z_2$$
\vspace{1cm}
$$|\overrightarrow{\scriptr}|=\scriptr=\sqrt{\overrightarrow{\scriptr}\cdot \overrightarrow{\scriptr}}= \sqrt{x^2+y^2+z^2}$$
\vspace{1cm}
$$\overrightarrow{\scriptr_1}\cdot \overrightarrow{\scriptr_2}= \scriptr_1\scriptr_2 \cos \gamma$$
\vspace{1cm}
$$
\begin{tikzpicture}[scale=1.2,tdplot_main_coords,decoration={brace,amplitude=2pt}]
\draw [thick,->] (0,0) -- (1,2) node[anchor=south,color=black]{$\overrightarrow{\scriptr_2}$};
\draw [thick,->] (0,0) -- (2,0) node[anchor=south east,color=black]{$\overrightarrow{\scriptr_1}$};
\tdplotdrawarc{(0,0)}{0.8}{0}{60}{anchor=north}{$\gamma$}
\end{tikzpicture}
$$

\subsection{Cross Product}
\vspace{1cm}
$$\overrightarrow{\scriptr_1}\times\overrightarrow{\scriptr_2}=\begin{vmatrix}
\hat{x}&\hat{y}&\hat{z}\\
x_1&y_1&z_1\\
x_2&y_2&z_2\\
\end{vmatrix}=(y_1z_2-z_1y_2)\hat{x}+(z_1x_2-x_1z_2)\hat{y}+(x_1y_2-y_1x_2)\hat{z}$$
\vspace{1cm}
$$
\begin{tikzpicture}[scale=1.2,tdplot_main_coords,decoration={brace,amplitude=2pt}]
\draw [thick,->] (0,0,0) -- (1,2,0) node[anchor=south,color=black]{$\overrightarrow{\scriptr_2}$};
\draw [thick,->] (0,0,0) -- (2,0,0) node[anchor=south east,color=black]{$\overrightarrow{\scriptr_1}$};
\draw [thick,->] (0,0,0) -- (0,0,1.414) node[anchor=east,color=black]{$\overrightarrow{\scriptr_1}\times\overrightarrow{\scriptr_2}$};
\tdplotdrawarc{(0,0,0)}{0.8}{0}{60}{anchor=north}{$\gamma$}
\draw  (.4,0,0) -- (.4,0,.4) -- (0,0,.4);
\draw  (0.133,.266,0) -- (0.133,.266,.4) -- (0,0,.4);
\end{tikzpicture}
$$
\vspace{1cm}
$$|\overrightarrow{\scriptr_1}\times\overrightarrow{\scriptr_2}|=\scriptr_1\scriptr_2 \sin \gamma$$

