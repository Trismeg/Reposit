  \draw [decorate,color=black!80!black] (0.75,-0.02) -- (0.25,-0.02)
   node [midway,anchor=north,inner sep=1pt, outer sep=1pt]{\small$\Delta x$};
 \end{scope}
 
 \draw[dotted,color=blue!30, very thick] (0.25,0.1224) -- (0,0.1224);
