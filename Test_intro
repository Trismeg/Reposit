\newpage

\section{Review}
\ \ 
\vspace{1cm}
$$\forall n \in \mathbb{R}: d(x^n)=n\  x^{n-1} dx$$
$$d(\ln x )=\frac{dx}{x}$$
$$d(e^x)=e^x\ dx $$
$$d(\cos x)=-\sin x \ dx $$
$$d(\sin x)=\cos x\ dx $$
\vspace{1cm}
\subsection{Product Rule}
\vspace{1cm}
$$d (f\cdot g)=f\cdot d g+g\cdot df$$
$$
\begin{tikzpicture}
      [decoration={brace,amplitude=2pt}, scale=0.75]%main layer

%main layer
%creating the ticks and xy-axis nodes
  \draw[color=black, thick] (-2,-2) -- (2,-2) -- (2,2) -- (-2,2)--cycle;

  \draw[color=black] (2,-2)--(2.5,-2) -- (2.5,2)-- (2,2);
     \draw[color=black] (2.5,-2.1) -- (2.5,-2.5);
      \draw[color=black] (2,-2.1) -- (2,-2.5);
       \draw[-latex,color=black] (1.5,-2.3)--(2,-2.3);
        \draw[-latex,color=black] (3,-2.3)--(2.5,-2.3);
        
      \draw[color=black] (-2,2)--(-2,2.5) -- (2,2.5)-- (2,2);
     \draw[color=black] (-2.1,2.5) -- (-2.5,2.5);
      \draw[color=black] (-2.1,2) -- (-2.5,2);
       \draw[-latex,color=black] (-2.3,1.5)--(-2.3,2);
        \draw[-latex,color=black] (-2.3,3)--(-2.3,2.5);    
    
     \draw (0,0) node [anchor=center,scale=1] {$f\cdot g$};
     \draw (-2,0) node [anchor=east,scale=1] {$f$};
         \draw (0,-2) node [anchor=north,scale=1] {$g$};
         \draw (2.25,-2) node [anchor=north,scale=0.8] {$dg$};
         \draw (-2,2.25) node [anchor=east,scale=0.8] {$df$};
         \draw (0,2.25) node [anchor=center,scale=0.8] {$g\cdot df$};
          \draw (2.25,0) node [anchor=center,rotate=-90,scale=0.8] {$f\cdot dg$};
   
\end{tikzpicture}
$$
\vspace{1cm}
\subsection{Chain Rule}
\vspace{1cm}
$$d f(g)= f^\prime(g)\cdot dg $$
$$d g(x)= f^\prime(g)\cdot dx$$
$$d f(g(x))= f^\prime(g(x))\cdot g^\prime(x)\cdot dx$$








\section{Scalars and Vectors}
\vspace{1cm}
$$\overrightarrow{x}=x\hat{x}$$
$$|\overrightarrow{x}|=x$$
$$\hat{x}=\frac{\overrightarrow{x}}{x}$$
$$|\hat{x}|=1$$
\vspace{1cm}
$$
\begin{tikzpicture}[scale=3,decoration={brace,amplitude=2pt}]



%draw a vector from origin to point (P) 
\begin{scope}[rotate=40]
\draw[-stealth,very thick,color=red] (0,0) -- (1,0) node[anchor=south,color=black]{$\overrightarrow{x}$};
  \draw[->,thick,color=black] (0,0.07) -- (0.25,0.07) node[anchor=south east,color=black]{$\hat{x}$};
   \draw [decorate,color=black] (1,-0.07) -- (0,-0.07) 
   node [midway,anchor=north west,inner sep=2pt, outer sep=2pt]{$x$};
   \end{scope}



\end{tikzpicture}
$$

\section{Spatial Coordinate Systems}
\subsection{2D Cartesian $(x,y)$}
\vspace{1cm}
$$\overrightarrow{r}=\overrightarrow{y}+\overrightarrow{y}=x\hat{x}+y\hat{y}=r_x\hat{x}+r_y\hat{y}$$
$$ \overrightarrow{r}=\left(\begin{array}{c} x \\ y \end{array}\right)=\left(\begin{array}{c} r_x \\ r_y \end{array}\right)$$

\vspace{1cm}
$$
\begin{tikzpicture}[scale=1.5]


\draw[thick,->] (0,0,0) -- (3,0,0) node[anchor=north east]{$x$};
\draw[thick,->] (0,0,0) -- (0,3,0) node[anchor=north west]{$y$};


%draw a vector from origin to point (P) 
\draw[-stealth,very thick,color=red] (0,0) -- (1,2) node[anchor=south west,color=black]{$\overrightarrow{r}$};


%draw projection on xy plane, and a connecting line
\draw[dashed, color=red] (1,0) -- (1,2) node[midway,anchor=west ,scale=1,color=black] {$r_y$};
\draw[dashed, color=red] (0,2) -- (1,2) node[midway,anchor=south ,scale=1,color=black] {$r_x$};
\end{tikzpicture}
$$
\newpage
\subsection{Polar Coordinates $(r,\theta)$ }
\vspace{1cm}
$$\overrightarrow{r}=r\hat{r}$$
\vspace{1cm}
$$r=\sqrt{x^2+y^2}$$
$$\theta=\tan^{-1}\left(\frac{y}{x}\right)$$
\vspace{1cm}

$$
\begin{tikzpicture}[scale=1.5]


\draw[thick,->] (0,0,0) -- (3,0,0) node[anchor=north east]{$x$};
\draw[thick,->] (0,0,0) -- (0,3,0) node[anchor=north west]{$y$};


%draw a vector from origin to point (P) 
\draw[-stealth,very thick,color=red] (0,0) -- (1,2) node[anchor=south west,color=black]{$\overrightarrow{r}$};
\draw[color=red] (0,0) -- (0.5,1) node[anchor=south east ,color=black]{$r$};


%draw projection on xy plane, and a connecting line
\draw (0.75,0) arc (0:60:0.75) ;
\draw (0.25,0.1) node [anchor=south west,color=black]{$\theta$};
\end{tikzpicture}
$$
\vspace{1cm}
$$x=r\cos\theta$$
$$y=r\sin\theta$$
\vspace{1cm}
$$\hat{r}=\frac{\overrightarrow{r}}{r}=\frac{x\hat{x}+y\hat{y}}{\sqrt{x^2+y^2}}=\frac{r\cos \theta\hat{x}+r\sin \theta\hat{y}}{\sqrt{(r\cos \theta)^2+(r\sin \theta)^2}}=\cos \theta\hat{x}+\sin \theta\hat{y}$$

\newpage

\subsection{3D Cartesian $(x,y,z)$}
\vspace{1cm}
$$\overrightarrow{\scriptr}=\overrightarrow{x}+\overrightarrow{y}+\overrightarrow{z}=x\hat{x}+y\hat{y}+z\hat{z}=\scriptr_x\hat{x}+\scriptr_y\hat{y}+\scriptr_z\hat{z}$$
$$ \overrightarrow{\scriptr}=\left(\begin{array}{c} x \\ y \\ z\end{array}\right)=\left(\begin{array}{c} \scriptr_x \\ \scriptr_y \\ \scriptr_z\end{array}\right)$$

